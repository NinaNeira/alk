<div class="rooms">
    <h1>Pokoje</h1>
    <div class="col-12">
        <div class="form-group">
            <input type="text" class="form-control clickable input-md rooms__date--arr" data-date-start-date="0d"
                placeholder="&#xf073; &#xf5af;">
        </div>
        <div class="form-group">
            <input type="text" class="form-control clickable input-md rooms__date--dep" id="DtChkOut"
                data-date-start-date="0d" data-date-end-date="+365d" placeholder="&#xf073; &#xf5b0;">
        </div>
        <div class="row">
            {{#each data}}
            <div class="col-3">
                <div class="card">
                    <img class="card-img-top" src=".../100px180/" alt="Card image cap">
                    <div class="card-body">
                        <h5 class="card-title">{{name}}</h5>
                        <p class="card-text">Beds: {{beds}}<br />
                            Guests: {{guests}}<br />
                            Price: {{price}}</p>
                        <a class="btn btn-primary" data-id="{{_id}}">Go somewhere</a>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>